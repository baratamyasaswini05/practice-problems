import scala.util.control.Breaks._
object Solution {

    def main(args: Array[String]) {
        var s=scala.io.StdIn.readLine().split(" ")
        var (n,res)=(s(0).toInt,s(1).toInt)
        var c=0
        for(i<- 1 to n){
            if(res%i==0 && (res/i)<=n) c+=1
        }
        println(c)
    }
}
